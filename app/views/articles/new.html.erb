<div class="container">
  <%= form_for(@article, url: { action: "create" }) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :url %>
    <%= f.text_field :url %>
    <%= f.label :language %>
    <%= f.text_field :language %>
    <%= f.submit "Add Article" %>
  <% end %>
</div>
